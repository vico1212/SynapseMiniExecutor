	wait(10)
	fil:TweenPosition(UDim2.new(-0.161, 0,0.883, 0),"Out","Linear",1,false,nil)
	msg:TweenPosition(UDim2.new(0.171, 0,0.794, 0),"Out","Linear",1,false,nil)
