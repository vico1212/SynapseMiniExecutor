	script.Parent.StartupFrame:TweenSize(UDim2.new(0,0),"In","Linear",0.3,false,nil)
	script.Parent.StartupFrame.Top:TweenSize(UDim2.new(0,0),"In","Linear",0.3,false,nil)
	script.Parent.StartupFrame.LoadingBar:TweenSize(UDim2.new(0,0),"In","Linear",0.3,false,nil)
	script.Parent.StartupFrame.Message:TweenSize(UDim2.new(0,0),"In","Linear",0.3,false,nil)
	script.Parent.StartupFrame.Message.Status.Text = ""
	script.Parent.StartupFrame.Message.StatusText.Text = ""
	script.Parent.StartupFrame.NotLong.Text = ""
	script.Parent.StartupFrame.initializing.Text = ""
	script.Parent.StartupFrame.Top.Scripting.Text = ""
	script.Parent.StartupFrame.Top.SynapseLogo:TweenSize(UDim2.new(0,0),"In","Linear",0.3,false,nil)
	bar.Bal.GreenFilling:TweenSize(UDim2.new(0,0),"In","Linear",0.3,false,nil)
	bar.Bal2.GreenFilling:TweenSize(UDim2.new(0,0),"In","Linear",0.3,false,nil)
	bar.Bal3.GreenFilling:TweenSize(UDim2.new(0,0),"In","Linear",0.3,false,nil)
	bar.Bal4.GreenFilling:TweenSize(UDim2.new(0,0),"In","Linear",0.3,false,nil)
	bar.Bal:TweenSize(UDim2.new(0,0),"In","Linear",0.3,false,nil)
	bar.Bal2:TweenSize(UDim2.new(0,0),"In","Linear",0.3,false,nil)
	bar.Bal3:TweenSize(UDim2.new(0,0),"In","Linear",0.3,false,nil)
	bar.Bal4:TweenSize(UDim2.new(0,0),"In","Linear",0.3,false,nil)
	wait(2)
	script.Parent.Main:TweenPosition(UDim2.new(0.246, 0,0.259, 0),"Out","Linear",1.2,false,nil)
end
